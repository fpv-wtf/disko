+static void tuner_dispose(tuner_t * this)
+{
+	int i;
+
+	if(this->fd_frontend >= 0) {
+		net_close(this->fd_frontend);
+	}
+
+	/* close all pid filter filedescriptors */
+	for(i = 0; i < MAX_FILTERS; ++i) {
+		if(this->fd_pidfilter[i] >= 0) {
+			net_close(this->fd_pidfilter[i]);
+		}
+	}
+
+	if(this) {
+		free(this);
+	}
+}
